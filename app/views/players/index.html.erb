<div class="row">
  <div class="span7">
    <table class="table table-hover">
      <thead>
      <tr>
        <th>Player Rank</th>
        <th>Position</th>
        <th>Name</th>
        <th>Team</th>
        <th>Projection</th>
      </tr>
      </thead>
      <tbody>
      <% @players.each do |player| %>
          <% @rank += 1 %>
          <tr>
            <td>
              <%= @rank %>
            </td>
            <td>
              <%= player.position %>
            </td>
            <td>
              <%= player.name %>
            </td>
            <td>
              <%= player.yahoo_team_name %>
            </td>
            <td>
              <%= player.points %>
            </td>
          </tr>
      <% end %>
      </tbody>
    </table>
  </div>
</div>
